{
  "name": "React-es6-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --history-api-fallback --inline --progress --colors --port 3000 --config webpack.dev.js",
    "start:devwrite": "webpack-dev-server --history-api-fallback --inline --progress --colors --port 3000 --config webpack.dev.write.js",
    "build:stats": "webpack --env production --profile --progress --json > stats.json",
    "build": "npm run clean && npm run build:prod",
    "build:prod": "webpack --config webpack.prod.js",
    "dll": "webpack --config webpack.dll.config.babel.js",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.82.0",
    "uuid": "^3.2.1",
    "validatorjs": "^3.14.2"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.3",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^1.0.0",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^4.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss-assets": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-fontpath": "^1.0.0",
    "postcss-loader": "^2.1.5",
    "postcss-media-minmax": "^3.0.0",
    "postcss-reporter": "^5.0.0",
    "react-hot-loader": "^4.3.3",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.6",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "write-file-webpack-plugin": "^4.3.2"
  }
}
